/* pages/calendarsign/calendarsign.wxss */
.calendar{
    margin:0 1vw;
}
.header{
    text-align:center;
    margin:20rpx 0;
}
.t_red{
    color:red;
}
.t_blue{
    color:royalblue;
}
.time{
    padding:16rpx 20rpx;
    background-color:#FFF68F;
    display:flex;
}
.time view{
    flex:1;
    font-size:30rpx;
}
.time view text{
    font-size:38rpx;
}
.weekName{
    width:100%;
    display:flex;
    padding:16rpx 0;
    background-color:#EEE9BF;
}
.weekName view{
    flex:1;
    text-align:center;
}
.week{
    width:100%;
}
.week view{
    display: inline-block;
    margin:20rpx 0;
    width:14.2%;
    height:80rpx;
    line-height:80rpx;
    text-align: center;
}
.week view text{
    width:100%;
    height:100%;
    display: inline-block;
}
.calendarSign{
    margin-top:20rpx;
}